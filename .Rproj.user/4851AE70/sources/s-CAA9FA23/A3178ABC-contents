#set working directory
setwd(paste0(Sys.getenv("HOME"), "/Map_madagascar"))
library(raster)

r <- raster("Final2/Prevalence_surfaces/prevalence_2016.tif")

# plot(r)

districts <- shapefile("District_shapefile/Malareo_District.shp")
CHL_no <- c(1:23, 35, 19, 26)
CHL <- districts[CHL_no,]

# plot(districts)
# plot(districts[CHL_no, ], col="red", add=TRUE)

r_CHL <- crop(r, extent(CHL))

plot(r_CHL)
